<div class="container">
    <div class="users row">
        <% @users.each do |user| %>
       
        <% if user != current_user && !current_user.friends.include?(user) %> 
         
        
        <div class="panel panel-default">
            <%= link_to user_path(user) do %>
            <div class="panel-body">
               
                <p><%= user.fullname %></p>
                <p> <%= link_to "Add Friend", friendships_path(:friend_id => user), :method => :post %></p>

            </div>
            <% end %>
            <% end %>
        </div>
        <% end %>
    </div>
</div>